@{
    int i = 0;
}
<h1>Оценки</h1>

<table>
    <thead>
        <td>№</td>
        <td>Группа</td>
        <td>Предмет</td>
        <td>Тип</td>
        <td>Выбор</td>
    </thead>

    @foreach (var it in @ViewBag.Exams)
   {
       <tr>
           <td>@(i = i + 1)</td>
           <td>@it.NameGroup</td>
           <td>@it.NamePredmet</td>
           <td>@it.NameType</td>
           <td><a href="~/Teacher/SetGrade2?id=@it.ID">=></a></td>
       </tr>
   }
</table>
